@model Portfolio.Models.PortfolioWebModel

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<div class="content-wrapper">
        <h1>새로운 웹 포트폴리오 작성</h1>
        <div class="card-body">


        <div class="row">
            <div class="col-md-4">
                <form asp-action="Create" class="forms-sample">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                    <div class="form-group" hidden>
                        <label asp-for="Id" class="control-label"></label>
                        <input asp-for="Id" class="form-control" />
                        <span asp-validation-for="Id" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Title" class="control-label"></label>
                        <input asp-for="Title" class="form-control" />
                        <span asp-validation-for="Title" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Url" class="control-label"></label>
                        <input asp-for="Url" class="form-control" />
                        <span asp-validation-for="Url" class="text-danger"></span>
                    </div>
@*                    <div class="form-group" hidden>
                        <label asp-for="CurrentTime" class="control-label"></label>
                        <input asp-for="CurrentTime" class="form-control" />
                        <span asp-validation-for="CurrentTime" class="text-danger"></span>
                    </div>*@
                    <div class="form-group">
                        <input type="submit" value="생성하기" class="btn btn-primary" />
                    </div>
                </form>
            </div>
        </div>
    </div>

